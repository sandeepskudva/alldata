package webAccountCreation;

import java.util.NoSuchElementException;

public class whileloop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int flag = 1;
		
		while(flag==1)
		{
			try {
				
			}
			catch( NoSuchElementException e ) {
	            e.printStackTrace();
	            
	            flag =0;
	            
			}	
			
			
		}

	}
}
	


